{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": [
      "error",
      "unix"
    ],
    "semi": [
      "warn",
      "never"
    ],
    "no-console": [
      "error",
      {
        "allow": [
          "log","warn"
        ]
      }
    ]
  }
}